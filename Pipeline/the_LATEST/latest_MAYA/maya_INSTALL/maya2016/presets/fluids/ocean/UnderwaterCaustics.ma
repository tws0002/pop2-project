	setAttr ".wop" -type "float3" 0.62699997 0.37055695 0.37055695 ;
	setAttr ".wlv" 6;
	setAttr ".wdp" 6;
	setAttr ".wdc" 0.2912;
	setAttr ".wlc" 0.68932000000000004;
createNode lightLinker -n "lightLinker1";
	setAttr ".ihi" 0;
	setAttr -s 4 ".lnk";
