	setAttr ".ixp" -type "string" ".O[0]=time";
createNode materialInfo -n "materialInfo2";
createNode shadingEngine -n "envFogSE";
	setAttr ".ihi" 0;
	setAttr ".ro" yes;
createNode envFog -n "envFogMaterial";
