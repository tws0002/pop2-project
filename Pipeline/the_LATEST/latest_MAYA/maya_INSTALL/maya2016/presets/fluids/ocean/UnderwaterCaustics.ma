	setAttr ".c" -type "float3" 0.76862746 0.76862746 0.76862746 ;
	setAttr ".ic" -type "float3" 0.015686274 0.015686274 0.015686274 ;
createNode ocean -n "ocean1";
	addAttr -ci true -sn "nts" -ln "notes" -dt "string";
	setAttr ".cg" -type "float3" 0.90667534 0.98400003 0.90134406 ;
	setAttr ".co" -type "float3" 0.101083 0.17004101 0.271 ;
