	setAttr ".wtb[0]"  0 1 1;
	setAttr ".wp[0]"  0 1.5 1;
	setAttr ".fmt" 0.5339999794960022;
	setAttr ".nts" -type "string" (
		"The wave peaking on the ocean texture is set to a high value to create a caustic fake that works well for shallow water. One could customize a ocean texture used for a water surface displacement to create a matching caustic in this manner. This caustic effect is projected down the light direction by mapping the light color. Simply place any objects into this scene to look like they are underwater.\r\n"
		+ "Render time: $");
createNode place2dTexture -n "place2dTexture1";
createNode expression -n "expression1";
