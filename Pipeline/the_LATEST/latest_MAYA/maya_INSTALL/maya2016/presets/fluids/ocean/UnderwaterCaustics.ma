	setAttr -k off ".v";
	setAttr ".cl" -type "float3" 0.010527998 0.11733516 0.22400001 ;
	setAttr ".as" 0.10679999738931656;
createNode transform -n "nurbsPlane1";
	setAttr ".s" -type "double3" 330.53042796348876 330.53042796348876 330.53042796348876 ;
